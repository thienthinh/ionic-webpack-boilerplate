{
  "name": "ionic-webpack-boilerplate",
  "version": "1.1.1",
  "description": "ionic-webpack-boilerplate: An Ionic project",
  "scripts": {
    "dev": "webpack-dev-server --colors --json --progress",
    "dumpdev": "rm -rf www/* && webpack",
    "dumpprod": "rm -rf www/* && webpack --release --config webpack.config.prod.js",
    "runAndroid": "npm run dumpdev && ionic run android",
    "runIosEmulator": "npm run dumpdev && ionic run ios",
    "runIosDevice": "npm run dumpdev && ionic run ios --device",
    "platformAddAndroid": "ionic platform add android",
    "platformAddIOS": "ionic platform add ios"
  },
  "dependencies": {
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "ionic-sdk": "^1.2.4"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.2.2",
    "bower": "^1.3.3",
    "css-loader": "^0.9.1",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.6.0",
    "file-loader": "^0.8.1",
    "gulp-util": "^2.2.14",
    "html-loader": "^0.2.3",
    "html-webpack-plugin": "^1.5.0",
    "json-loader": "^0.5.1",
    "ng-annotate-loader": "~0.0.6",
    "node-sass": "^3.10.1",
    "path": "^0.4.9",
    "sass-loader": "^1.0.0",
    "shelljs": "^0.3.0",
    "style-loader": "^0.8.3",
    "webpack": "~1.10.0",
    "webpack-dev-server": "~1.10.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ]
}
